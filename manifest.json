{
  "manifest_version": 2,

  "name": "Miss Direction",
  "description": "This extension shows gets you to places....maybe",
  "version": "0.0",

  "content_scripts": [
    {
      "matches": ["http://www.google.com/maps/*"],
      "js": ["auto.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "background",
    "window",
    "https://ajax.googleapis.com/",
    "https://www.google.com/maps/*",
    "https://ashes74.github.io/miss-direction/*"
  ]
}
